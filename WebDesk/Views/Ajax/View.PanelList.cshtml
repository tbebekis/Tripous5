@model ViewPanelListModel

<div class='PanelList @Html.Raw(Model.Def.GetCssClassesText())' @Html.Raw(Model.Def.GetCssStyleText()) data-setup='@Html.Raw(Model.Def.GetDataSetupText())'>
    @foreach (var PanelDef in Model.Def.Items)
    {
        <div class="@Html.Raw(PanelDef.GetCssClassesText())" @Html.Raw(PanelDef.GetCssStyleText()) data-setup='@Html.Raw(PanelDef.GetDataSetupText())'>
            @{
                var ContainerPanelModel = new ViewContainerPanelModel(Model.ViewDef, PanelDef);
                @await Html.PartialAsync("View.ContainerPanel", ContainerPanelModel)
            }
        </div>
    }
</div>

